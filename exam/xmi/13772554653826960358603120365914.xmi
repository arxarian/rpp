xD;&#xA;    cTH.Print('@UmlParameter#fa83e476-385b-4e56-884b-474577e3ef2e@ is not of type String', 'ERROR')&#xD;&#xA;if(type(iPort)!= types.IntType):&#xD;&#xA;    cTH.Print('@UmlParameter#bdc8fd58-bad7-46db-a780-7a37d471c0fd@ is not of type Integer', 'ERROR')&#xD;&#xA;&#xD;&#xA;if not cTH.teachAndReplay.isReplayMode():&#xD;&#xA;    # normal or teach mode&#xD;&#xA;    &#xD;&#xA;    # create a server socket object&#xD;&#xA;    @UmlAttribute#5cf7fedf885d70e1e04400144f6890f0@ = socket.socket(socket.AF_INET, socket.SOCK_STREAM)&#xD;&#xA;    &#xD;&#xA;    # bind the socket object to specified address and port&#xD;&#xA;    @UmlAttribute#5cf7fedf885d70e1e04400144f6890f0@.bind((@UmlParameter#fa83e476-385b-4e56-884b-474577e3ef2e@, @UmlParameter#bdc8fd58-bad7-46db-a780-7a37d471c0fd@))&#xD;&#xA;    &#xD;&#xA;    # activate passive and listen for requests - request buffer = 5&#xD;&#xA;    @UmlAttribute#5cf7fedf885d70e1e04400144f6890f0@.listen(5) &#xD;&#xA;&#xD;&#xA;# log message&#xD;&#xA;cTH.Print(&quot;... server socket is listening (&quot; + str(@UmlParameter#fa83e476-385b-4e56-884b-474577e3ef2e@) + &quot;:&quot; + str(@UmlParameter#bdc8fd58-bad7-46db-a780-7a37d471c0fd@) + &quot;)&quot;, 'INFO')" defaultStepText="" ordinal="0" bodyRefersAttributes="33368" bodyRefersParameters="37514 40530">
