    <operationList xmi:id="35576" id="35576" uuid="ef191a7e-ebc1-4bb6-81c6-b6020e01420e" lastWriteDate="2013-07-22 15:51:06.0" name="sendFile" description="{\rtf1\ansi\ansicpg1252\deff0\deflang1031\deftab708{\fonttbl{\f0\fswiss\fprq2\fcharset0 Arial;}{\f1\fnil\fcharset0 Arial;}}&#xD;&#xA;\viewkind4\uc1\pard\lang1024\f0\fs20 For its description, please see the associated interface method.\lang1031\f1\par&#xD;&#xA;\par&#xD;&#xA;\pard\tx720\ul Details:\ulnone\par&#xD;&#xA;---\par&#xD;&#xA;}&#xD;&#xA;" beschreibung="{\rtf1\ansi\ansicpg1252\deff0\deflang1031\deftab708{\fonttbl{\f0\fnil\fcharset0 Arial;}}&#xD;&#xA;\viewkind4\uc1\pard\tx720\fs20 Beschreibung siehe zugeh\'f6rige Interface-Methode\par&#xD;&#xA;\par&#xD;&#xA;\ul Details:\ulnone\par&#xD;&#xA;---\par&#xD;&#xA;}&#xD;&#xA;" history="{\rtf1\ansi\ansicpg1252\deff0\deflang1031\deftab708{\fonttbl{\f0\fmodern\fprq1\fcharset0 Arial;}{\f1\fnil\fcharset0 Arial;}}&#xD;&#xA;\viewkind4\uc1\pard\f0\fs20 01.07.2010\tab kramer\tab\tab 2.1.0\tab revision for teach &amp; replay\f1\par&#xD;&#xA;23.03.2009\tab kramer\tab\tab 2.0.0\tab get number of chars in the receive buffer for read operation\par&#xD;&#xA;07.11.2008\tab kramer\tab\tab 1.0.1\tab revision for EXAM 2.0\par&#xD;&#xA;25.10.2007\tab sattler\tab\tab 1.0.0\tab programming version 1.0.0\par&#xD;&#xA;}&#xD;&#xA;" visibility="Public" abstract="false" static="true" pythonBody="'''&#xD;&#xA;    description: Send a file via RS232.&#xD;&#xA;'''&#xD;&#xA;&#xD;&#xA;# imports&#xD;&#xA;import time&#xD;&#xA;&#xD;&#xA;# log message&#xD;&#xA;cTH.Print(&quot;... send file via RS232: &quot; + @UmlParameter#dcbc0d96-9d4b-4c4c-95a3-a21501c9346b@, 'DEBUG')&#xD;&#xA;&#xD;&#xA;if cTH.teachAndReplay.isReplayMode():&#xD;&#xA;    # replay mode&#xD;&#xA;    &#xD;&#xA;    # get result from teach run&#xD;&#xA;    @UmlParameter#96d52d22-eee0-4622-9dad-d04420c28ace@ = cTH.teachAndReplay.replayData('@UmlParameter#96d52d22-eee0-4622-9dad-d04420c28ace@')&#xD;&#xA;else:&#xD;&#xA;    # normal or teach mode&#xD;&#xA;    &#xD;&#xA;    # open file&#xD;&#xA;    file = open(@UmlParameter#dcbc0d96-9d4b-4c4c-95a3-a21501c9346b@)&#xD;&#xA;    &#xD;&#xA;    # send file line per line via RS232&#xD;&#xA;    for sLine in file:&#xD;&#xA;        @UmlAttribute#2f9a1d5e-3e9d-4adf-8f51-67aeec85bd50@.write(sLine)&#xD;&#xA;    &#xD;&#xA;    # close file&#xD;&#xA;    file.close()&#xD;&#xA;    &#xD;&#xA;    # wait for response&#xD;&#xA;    time.sleep(1.0)&#xD;&#xA;    &#xD;&#xA;    # get the number of chars in the receive buffer&#xD;&#xA;    iChars = int(@UmlAttribute#2f9a1d5e-3e9d-4adf-8f51-67aeec85bd50@.inWaiting())&#xD;&#xA;    &#xD;&#xA;    # read characters in order to get the result&#xD;&#xA;    @UmlParameter#96d52d22-eee0-4622-9dad-d04420c28ace@ = @UmlAttribute#2f9a1d5e-3e9d-4adf-8f51-67aeec85bd50@.read(iChars)&#xD;&#xA;    &#xD;&#xA;    if cTH.teachAndReplay.isTeachMode():&#xD;&#xA;        # teach mode&#xD;&#xA;        &#xD;&#xA;        # teach result&#xD;&#xA;        cTH.teachAndReplay.teachData('@UmlParameter#96d52d22-eee0-4622-9dad-d04420c28ace@', @UmlParameter#96d52d22-eee0-4622-9dad-d04420c28ace@)" defaultStepText="" ordinal="2" bodyRefersAttributes="33378" bodyRefersParameters="37414 41834">
